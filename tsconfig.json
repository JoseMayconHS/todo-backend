{
	"compilerOptions": {
		"target": "esNext",
		"module": "commonjs",
		"baseUrl": "./",
		"paths": {
			"@repositories/*": ["src/repositories/*"],
			"@models/*": ["src/models/*"]
		},
		"resolveJsonModule": true,
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": false,
		"skipLibCheck": true
	},
	"exclude": ["node_modules", "dist", "src/**/*.spec.ts"],
	"include": ["src/**/*.ts"]
}
